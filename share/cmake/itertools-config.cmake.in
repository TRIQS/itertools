# This file allows other CMake Projects to find us
# We provide general project information
# and reestablish the exported CMake Targets

# Multiple inclusion guard
if(NOT ITERTOOLS_FOUND)
set(ITERTOOLS_FOUND TRUE)
set_property(GLOBAL PROPERTY ITERTOOLS_FOUND TRUE)

# version
set(ITERTOOLS_VERSION @PROJECT_VERSION@)
set(ITERTOOLS_GIT_HASH @PROJECT_GIT_HASH@)

# Root of the installation
set(ITERTOOLS_ROOT  @CMAKE_INSTALL_PREFIX@)

## Find the target dependencies
#include(CMakeFindDependencyMacro)
#find_dependency(...)

# Include the exported targets of this project
include(@CMAKE_INSTALL_PREFIX@/lib/cmake/itertools/itertools-targets.cmake)

message(STATUS "Found itertools-config.cmake with version @PROJECT_VERSION@, hash = @PROJECT_GIT_HASH@")

# Was the Project built with Documentation?
set(ITERTOOLS_WITH_DOCUMENTATION @Build_Documentation@)

endif()
